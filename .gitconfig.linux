[user]
	email = me@me.com
	name = me
[core]
	editor = code --wait
[push]
	default = simple
[alias]
	p = pull --prune
	co = checkout
	cob = checkout -b
	cm = commit -m
	cam = commit -am
	st = status
	lg = log --pretty=oneline --abbrev-commit --decorate --graph
	rb = rebase
	previewmerge = merge --no-commit --no-ff
[credential]
	helper = cache
