[user]
	name = me
	email = me@me.com
[core]
	editor = \"/c/Program Files/Sublime Text 3/sublime_text.exe\" -n -w
[push]
	default = simple
[alias]
	p = pull --prune
	co = checkout
	cob = checkout -b
	cm = commit -m
	cam = commit -am
	st = status
	lg = log --pretty=oneline --abbrev-commit --decorate --graph --all
	rb = rebase
[credential]
	helper = wincred
